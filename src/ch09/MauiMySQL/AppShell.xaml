<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiMySQL.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiMySQL"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Connect"
        ContentTemplate="{DataTemplate local:ConnectPage}"
        Route="ConnectPage" />
    <ShellContent
        Title="Read"
        ContentTemplate="{DataTemplate local:ReadPage}"
        Route="ReadPage" />
    </TabBar>

</Shell>
